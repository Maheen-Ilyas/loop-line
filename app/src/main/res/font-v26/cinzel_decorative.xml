<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <font
        android:font="@font/cinzel_decorative_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/cinzel_decorative_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        tools:ignore="ResourceCycle" />

    <font android:font="@font/cinzel_decorative_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/cinzel_decorative_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:ignore="ResourceCycle" />

    <font android:font="@font/cinzel_decorative_black"
        android:fontStyle="normal"
        android:fontWeight="900"
        app:font="@font/cinzel_decorative_black"
        app:fontStyle="normal"
        app:fontWeight="900"
        tools:ignore="ResourceCycle" />
</font-family>